{
  "name": "notes",
  "version": "1.0.0",
  "description": "Electron notes application with TypeScript and React",
  "main": "dist/main/main.js",
  "scripts": {
    "start": "electron .",
    "dev": "concurrently \"npm run watch\" \"npm run electron-dev\"",
    "electron-dev": "wait-on dist/main.js && electron .",
    "test": "npx playwright test",
    "test:jest": "npx jest",
    "build": "tsc && webpack --mode production",
    "watch": "concurrently \"tsc -w\" \"webpack --mode development --watch\"",
    "lint": "eslint --ext .ts,.tsx src/"
  },
  "keywords": [
    "electron",
    "typescript",
    "react",
    "notes"
  ],
  "author": "",
  "license": "MIT",
  "dependencies": {
    "@heroicons/react": "^2.2.0",
    "@reduxjs/toolkit": "^2.7.0",
    "electron": "^35.2.1",
    "electron-devtools-installer": "^4.0.0",
    "react": "^19.1.0",
    "react-dom": "^19.1.0",
    "react-markdown": "^10.1.0",
    "react-redux": "^9.2.0",
    "redux": "^5.0.1",
    "remark-gfm": "^4.0.1",
    "uuid": "^11.1.0"
  },
  "devDependencies": {
    "@playwright/test": "^1.52.0",
    "@tailwindcss/postcss": "^4.1.4",
    "@types/electron": "^1.4.38",
    "@types/jest": "^29.5.14",
    "@types/node": "^22.15.2",
    "@types/react": "^19.1.2",
    "@types/react-dom": "^19.1.2",
    "@types/uuid": "^10.0.0",
    "@typescript-eslint/eslint-plugin": "^8.31.0",
    "@typescript-eslint/parser": "^8.31.0",
    "autoprefixer": "^10.4.21",
    "concurrently": "^9.1.2",
    "css-loader": "^7.1.2",
    "eslint": "^9.25.1",
    "eslint-plugin-react": "^7.37.5",
    "eslint-plugin-react-hooks": "^5.2.0",
    "html-webpack-plugin": "^5.6.3",
    "jest": "^29.7.0",
    "postcss": "^8.5.3",
    "postcss-loader": "^8.1.1",
    "style-loader": "^4.0.0",
    "tailwindcss": "^4.1.4",
    "ts-jest": "^29.3.2",
    "ts-loader": "^9.5.2",
    "typescript": "^5.8.3",
    "wait-on": "^8.0.3",
    "webpack": "^5.99.6",
    "webpack-cli": "^6.0.1"
  }
}
